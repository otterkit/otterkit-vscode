{
	"name": "otterkit-vscode",
	"displayName": "Otterkit COBOL",
	"description": "Otterkit COBOL extension for VS Code",
	"version": "1.70.0",
	"publisher": "ktsnowy",
	"author": "KTSnowy",
	"keywords": [
		"Otterkit",
		"COBOL"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/otterkit/otterkit-vscode.git"
	},
	"icon": "assets/otterkit-logo.png",
	"galleryBanner": {
		"color": "#212328",
		"theme": "dark"
	},
	"engines": {
		"vscode": "^1.70.0"
	},
	"categories": [
